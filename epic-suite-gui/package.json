{
    "name": "nextchakra-starter",
    "version": "0.1.0",
    "private": true,
    "author": "sozonome",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "start": "next start",
        "test": "jest --watch ",
        "lint": "next lint",
        "lint:fix": "eslint src --fix && yarn format",
        "format": "prettier --write src",
        "up-latest": "yarn upgrade-interactive --latest",
        "release": "standard-version",
        "push-release": "git push --follow-tags origin main",
        "prepare": "husky install",
        "docs": "jsdoc -c jsdoc.json"
    },
    "dependencies": {
        "@chakra-ui/icons": "1.0.15",
        "@chakra-ui/react": "1.6.7",
        "@emotion/cache": "11.4.0",
        "@emotion/react": "11.4.1",
        "@emotion/server": "11.4.0",
        "@emotion/styled": "11.3.0",
        "@fontsource/lexend": "4.5.1",
        "@heroicons/react": "^2.0.13",
        "@ltd/j-toml": "^1.30.0",
        "@reduxjs/toolkit": "^1.8.2",
        "@types/react-leaflet": "2.8.2",
        "@types/react-redux": "^7.1.24",
        "@types/topojson": "3.2.3",
        "@types/topojson-client": "3.1.0",
        "@types/topojson-specification": "1.0.2",
        "@types/uuid": "8.3.3",
        "axios": "0.24.0",
        "chakra-react-select": "1.2.2",
        "date-fns": "2.28.0",
        "framer-motion": "4.1.17",
        "jsdoc": "^3.6.11",
        "json2csv": "5.0.6",
        "leaflet": "1.7.1",
        "lodash": "4.17.21",
        "moment": "2.29.1",
        "next": "11.1.2",
        "next-pwa": "5.3.1",
        "next-seo": "4.26.0",
        "plotly.js": "2.6.3",
        "prettier-eslint": "13.0.0",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-datepicker": "4.5.0",
        "react-dom": "17.0.2",
        "react-icons": "4.2.0",
        "react-leaflet": "3.2.1",
        "react-plotly.js": "2.5.1",
        "react-redux": "^8.0.2",
        "react-test-renderer": "^17.0.2",
        "redux": "^4.2.0",
        "topojson": "3.0.2",
        "topojson-client": "3.1.0",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "13.1.0",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@types/iarna__toml": "^2.0.2",
        "@types/react": "17.0.24",
        "better-docs": "^2.7.2",
        "commitlint": "13.1.0",
        "eslint": "7.32.0",
        "eslint-config-next": "11.1.2",
        "eslint-config-sznm": "0.1.7",
        "husky": "7.0.2",
        "jest": "^27.5.1",
        "jsdoc": "^3.6.11",
        "lint-staged": "11.1.2",
        "next-sitemap": "1.6.168",
        "prettier": "2.4.1",
        "standard-version": "9.3.1",
        "typescript": "4.4.3"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --max-warnings=0",
            "prettier --write"
        ],
        "src/**/*.{json,css,scss,md}": [
            "prettier --write"
        ]
    }
